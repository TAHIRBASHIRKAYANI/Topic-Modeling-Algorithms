                                                      <Projects rdf:ID="Companheiros_Virtuais_de_Aprendizado_com_Comportamento_Adaptado">
                                                        <hasGroupCIn rdf:resource="#Grupo_de_Engenharia_da_Computacao"/>
                                                        <TypeOfFinance
                                                         rdf:datatype=
                                                        "http://www.w3.org/2001/XMLSchema#string"
