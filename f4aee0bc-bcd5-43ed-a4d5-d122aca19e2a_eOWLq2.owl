<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns="http://www.cin.ufpe.br/~mlfc/onto/scienceOWLq.owl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xml:base="http://www.cin.ufpe.br/~mlfc/onto/scienceOWLq.owl">
  <owl:Ontology rdf:about=""/>
  <owl:Class rdf:ID="Enterprise">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:ID="Department"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="hasParts"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:about="#Department"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Organization"/>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:ID="Lab"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:ID="Sector"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hasParts"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hasParts"/>
        </owl:onProperty>
        <owl:someValuesFrom>
          <owl:Class rdf:ID="Section"/>
        </owl:someValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:ID="Research-Institute"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="University"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="College"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:about="#Organization">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:ID="Organization-Part"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hasParts"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  </owl:Class>
  <owl:Class rdf:ID="Special-Sponsor">
    <owl:disjointWith>
      <owl:Class rdf:ID="National-Sponsor"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="International-Sponsor"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Sponsor"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Research_Group_CIN"/>
  <owl:Class rdf:ID="Undergraduate-Course">
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class rdf:ID="Discipline-Undergraduate"/>
        </owl:allValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="hasDiscipline"/>
        </owl:onProperty>
      </owl:Restriction>
    </owl:equivalentClass>
    <owl:disjointWith>
      <owl:Class rdf:ID="Graduate-Course"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Course"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Software-Project">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Development-Project"/>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:ID="National-Sponsor-Project"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:ID="Collegiate">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="hasMember"/>
        </owl:onProperty>
        <owl:someValuesFrom>
          <owl:Class rdf:ID="Student"/>
        </owl:someValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="hasSpecialCommission"/>
        </owl:onProperty>
        <owl:someValuesFrom>
          <owl:Class rdf:ID="Special-Commission"/>
        </owl:someValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="isRepresentedBy"/>
        </owl:onProperty>
        <owl:someValuesFrom>
          <owl:Class rdf:about="#Department"/>
        </owl:someValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Location">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >This is a very simple ontology of places. The good advantages of it are the instances of all the countries and American states. A few countries are bound to incorrect continents, but we are reviewing them.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="School-Event">
    <owl:disjointWith>
      <owl:Class rdf:ID="Lecture-REVISION"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Conference"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Educational-Event"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Time"/>
  <owl:Class rdf:ID="Graduate-National">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#National-Sponsor-Project"/>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:ID="Undergraduate-National"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:ID="Book-Chapter">
    <owl:disjointWith>
      <owl:Class rdf:ID="Workshop-Paper"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Magazine-Article"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Journal-Article"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Conference-Paper"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Article-in-Book"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Part-Publication"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="isPartOf"/>
        </owl:onProperty>
        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:minCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#isPartOf"/>
        </owl:onProperty>
        <owl:allValuesFrom>
          <owl:Class rdf:ID="Book"/>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Research-Staff">
    <owl:disjointWith>
      <owl:Class rdf:ID="Researcher"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Technical-Staff"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Administrative-Staff"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Academic-Staff"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Permanent"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Month"/>
  <owl:Class rdf:about="#Course">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:ID="Discipline"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hasDiscipline"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Center">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    ></rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="#Organization-Part"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hasParts"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:ID="School"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Section"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Institute"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Sector"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Faculty"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#isPartOf"/>
        </owl:onProperty>
        <owl:someValuesFrom rdf:resource="#Organization"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:ID="Laboratory"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Organization-Part"/>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:ID="Research-Group"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Department"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:about="#Technical-Staff">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Permanent"/>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:about="#Researcher"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Research-Staff"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#Administrative-Staff"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Academic-Staff"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:ID="Graduate-Collegiate">
    <owl:disjointWith>
      <owl:Class rdf:ID="Undergraduate-Collegiate"/>
    </owl:disjointWith>
    <rdfs:subClassOf rdf:resource="#Collegiate"/>
  </owl:Class>
  <owl:Class rdf:ID="Enterprise-Sponsor">
    <owl:disjointWith>
      <owl:Class rdf:ID="Government-Sponsor"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#National-Sponsor"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Graduate">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >There are also changes from KA2. Graduate inherits from researcher and student. </rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Student"/>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:ID="Undergraduate"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:about="#National-Sponsor-Project">
    <owl:disjointWith rdf:resource="#Software-Project"/>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Development-Project"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Teaching-Assistent">
    <owl:disjointWith>
      <owl:Class rdf:ID="Professor"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Lecturer"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Faculty-Staff"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Academic-Staff"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#Research-Institute">
    <owl:disjointWith>
      <owl:Class rdf:about="#University"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Lab"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Enterprise"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#College"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
              <owl:Class rdf:about="#Research-Group"/>
              <owl:Class rdf:about="#Department"/>
            </owl:unionOf>
          </owl:Class>
        </owl:allValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hasParts"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="#Organization"/>
  </owl:Class>
  <owl:Class rdf:about="#Graduate-Course">
    <rdfs:subClassOf rdf:resource="#Course"/>
    <owl:disjointWith rdf:resource="#Undergraduate-Course"/>
  </owl:Class>
  <owl:Class rdf:ID="Discipline-Professional">
    <rdfs:subClassOf rdf:resource="#Discipline"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#Discipline-Undergraduate"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Discipline-Graduate"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:ID="Academic">
    <rdfs:subClassOf rdf:resource="#Graduate"/>
    <owl:disjointWith>
      <owl:Class rdf:ID="Professional"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:about="#Magazine-Article">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Part-Publication"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class rdf:ID="Magazine"/>
        </owl:allValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#isPartOf"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:about="#Workshop-Paper"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Journal-Article"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Conference-Paper"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Book-Chapter"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#Article-in-Book"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:ID="PhD-Thesis">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="authors"/>
        </owl:onProperty>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class rdf:ID="PhD-Student"/>
        </owl:allValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#authors"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Thesis"/>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:ID="Master-Thesis"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:ID="Specialization">
    <owl:disjointWith>
      <owl:Class rdf:ID="Master-Professional"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Professional"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Workshop">
    <owl:disjointWith>
      <owl:Class rdf:about="#Conference"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#isPartOf"/>
        </owl:onProperty>
        <owl:allValuesFrom>
          <owl:Class rdf:about="#Conference"/>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Live-Scientific-Event"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#Book">
    <owl:disjointWith>
      <owl:Class rdf:ID="Scientific-Publication-Event"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Proceedings"/>
    </owl:disjointWith>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Note that 'has-Parts' slot has to be filled only with Book-Chapter and Article-in-Book instances.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hasParts"/>
        </owl:onProperty>
        <owl:allValuesFrom>
          <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
              <owl:Class rdf:about="#Book-Chapter"/>
              <owl:Class rdf:about="#Article-in-Book"/>
            </owl:unionOf>
          </owl:Class>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Dividable-Publication"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#Academic-Staff">
    <owl:disjointWith>
      <owl:Class rdf:about="#Researcher"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Technical-Staff"/>
    <owl:disjointWith rdf:resource="#Research-Staff"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#Administrative-Staff"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Permanent"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#Part-Publication">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The filling stuff or Dividable-Publication; it has a 'is-Part-Of' slot for Dividable-Publications, and number of initial and final pages of them. </rdfs:comment>
    <owl:disjointWith>
      <owl:Class rdf:ID="Project-Report"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Technical-Report"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Thesis"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Dividable-Publication"/>
    </owl:disjointWith>
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Class rdf:ID="Scientific_Document"/>
          <owl:Restriction>
            <owl:allValuesFrom>
              <owl:Class rdf:about="#Dividable-Publication"/>
            </owl:allValuesFrom>
            <owl:onProperty>
              <owl:ObjectProperty rdf:about="#isPartOf"/>
            </owl:onProperty>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:ID="Scientific_Event">
    <owl:equivalentClass>
      <owl:Class rdf:ID="Event"/>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:ID="Discipline-Instances-Undergraduate">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Discipline-Undergraduate"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Research_Topic">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hasParts"/>
        </owl:onProperty>
        <owl:allValuesFrom rdf:resource="#Research_Topic"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#Research_Topic"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#isPartOf"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Subject areas. With Has-Part-Of and Is-Part-Of slots, an hierarchy can be created.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#Conference-Paper">
    <rdfs:subClassOf rdf:resource="#Part-Publication"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#isPartOf"/>
        </owl:onProperty>
        <owl:allValuesFrom>
          <owl:Class rdf:about="#Proceedings"/>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:minCardinality>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#isPartOf"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:about="#Workshop-Paper"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Magazine-Article"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#Journal-Article"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Book-Chapter"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#Article-in-Book"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:ID="External-Member">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="External"/>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:ID="Special-Student"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Visitor"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:ID="Web-Page">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >This is a first ontology on Web, containing concepts such as Page, Anchors, and concepts useful to the classification and extraction of pages. It aims at treating the data on the pages and can be extended in other directions. The Web-Page class is based on java.net.URLConnection class. </rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#Project-Report">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="describesProject"/>
        </owl:onProperty>
        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:minCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Scientific_Document"/>
    </rdfs:subClassOf>
    <owl:disjointWith rdf:resource="#Part-Publication"/>
    <owl:disjointWith>
