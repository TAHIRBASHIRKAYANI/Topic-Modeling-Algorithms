            <coordinate>
              <Projects rdf:ID="Spaecemat">
                <isSponsoredBy>
                  <Sponsor rdf:ID="Governo_Ceará">
                    <sponsores rdf:resource="#Spaecemat"/>
                    <Name rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                    >Governo_Ceará</Name>
