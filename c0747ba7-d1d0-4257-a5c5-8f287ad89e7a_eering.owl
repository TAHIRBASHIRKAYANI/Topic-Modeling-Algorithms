        <rdfs:label xml:lang="fr"
            >Document d&#39;analyse</rdfs:label>
        <rdfs:subClassOf rdf:resource="#Document"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#respects"/>
                <owl:someValuesFrom rdf:resource="#IEEE_Analysis_document_standard"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="fr"
            >Document d&#233;crivant diverses informations &#224; propos d&#39;un logiciel &#224; concevoir.</rdfs:comment>
    </owl:Class>
    


